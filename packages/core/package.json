{
  "name": "@johnsonhuang4396/api-smasher",
  "type": "module",
  "version": "0.0.6",
  "description": "Convert Swagger documentation to TypeScript API files",
  "author": "Johnson Huang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnsonhuang4396/api-smasher.git"
  },
  "keywords": [
    "swagger",
    "typescript",
    "api",
    "generator"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./*": "./*"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*/": {
      "*/": [
        "dist/index.d.ts",
        "dist/index.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --watch",
    "build": "unbuild",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "ejs": "^3.1.10",
    "pathe": "^2.0.3",
    "typescript": "catalog:"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "^3.0.9",
    "fs-extra": "^11.3.0",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  }
}
