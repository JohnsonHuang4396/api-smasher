{
  "name": "@johnsonhuang4396/api-smasher",
  "type": "module",
  "version": "0.0.0",
  "description": "Convert Swagger documentation to TypeScript API files",
  "author": "Johnson Huang",
  "license": "MIT",
  "keywords": [
    "swagger",
    "typescript",
    "api",
    "generator"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "ejs": "^3.1.10",
    "pathe": "^2.0.3",
    "typescript": "catalog:"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "^3.0.9",
    "tsup": "catalog:",
    "vitest": "catalog:"
  }
}
