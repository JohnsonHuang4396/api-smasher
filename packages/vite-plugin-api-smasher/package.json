{
  "name": "@johnsonhuang4396/vite-plugin-api-smasher",
  "type": "module",
  "version": "0.0.0",
  "description": "Vite plugin for API Smasher",
  "author": "Johnson Huang",
  "license": "MIT",
  "keywords": [
    "vite-plugin",
    "swagger",
    "typescript",
    "api",
    "generator"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./*": "./*"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*/": {
      "*/": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nr stub && nr dev:client",
    "dev:client": "vite build src/client --watch",
    "stub": "unbuild --stub",
    "build": "rimraf dist && pnpm run --sequential /^build:/",
    "build:js": "unbuild",
    "build:client": "vite build src/client"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@johnsonhuang4396/api-smasher": "workspace:*",
    "@tanstack/vue-query": "^5.69.0",
    "@vueuse/core": "catalog:",
    "vue": "catalog:",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.321",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "^5.2.3",
    "ansis": "^3.17.0",
    "codemirror-theme-vars": "^0.1.2",
    "naive-ui": "^2.41.0",
    "sirv": "^3.0.1",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "unocss": "catalog:",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "catalog:",
    "unplugin-vue-router": "^0.12.0",
    "vfonts": "^0.0.3",
    "vite": "catalog:",
    "vite-dev-rpc": "^1.0.7",
    "vite-hot-client": "^2.0.4"
  }
}
